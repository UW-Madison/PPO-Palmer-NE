@article{giacomini_variable_2019,
  langid = {english},
  title = {Variable {{Inheritance}} of {{Amplified EPSPS Gene Copies}} in {{Glyphosate}}-{{Resistant Palmer Amaranth}} ({{{\emph{Amaranthus}}}}{\emph{ Palmeri}})},
  volume = {67},
  issn = {1550-2759},
  doi = {10.1017/wsc.2018.65},
  abstract = {Glyphosate-resistant (GR) Palmer amaranth (Amaranthus palmeri S. Watson) is considered one of the most troublesome weeds in the southern and central United States, but results of previous research to determine the mode of inheritance of this trait have been conflicting and inconclusive. In this study, we examined segregation patterns of EPSPS gene-copy numbers in F1 and F2 generations of A. palmeri and found no evidence of a Mendelian single-gene pattern of inheritance. Transgressive segregation for copy number was exhibited by several F1 and all of the F2 families, most likely the product of EPSPS copy-number variation within each plant. This variation was confirmed by assaying gene-copy number across clonal generations and among individual shoots on the same plant, demonstrating that EPSPS amplification levels vary significantly within a single plant. Increases and decreases in copy number occurred in a controlled, stress-free environment in the absence of glyphosate, indicating that EPSPS gene amplification is a random and variable process within the plant. The ability of A. palmeri to gain or lose EPSPS gene copies is a valuable adaptive trait, allowing this species to respond rapidly to selection pressures and changing environments.},
  number = {2},
  journaltitle = {Weed Science},
  date = {2019-03},
  pages = {176-182},
  keywords = {Davis,Marie A. Jasieniuk,Non-Mendelian inheritance,somaclonal variation,somatic mosaicism,transgressive segregation,University of California},
  author = {Giacomini, Darci A. and Westra, Philip and Ward, Sarah M.},
  file = {/Users/maxwelco/Zotero/storage/7ARKIXHS/262124DF992AD819B4695C88785CDF58.html}
}

