  journaltitle = {Weed Science},
  date = {2014-06},
  pages = {370-381},
  keywords = {Ambrosia trifida L.,giant ragweed,Glyphosate,glyphosate-resistant,Glyphosate-susceptible,metagenomics,next-generation sequencing,soil microorganisms},
